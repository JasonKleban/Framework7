{
  "name": "framework7",
  "version": "2.0.0-beta.1",
  "description": "Full featured mobile HTML framework for building iOS & Android apps",
  "main": "dist/js/framework7.js",
  "jsnext:main": "dist/js/framework7.module.js",
  "module": "dist/js/framework7.module.js",
  "scripts": {
    "build": "cross-env NODE_ENV=development ./node_modules/.bin/webpack",
    "dist": "cross-env NODE_ENV=production ./node_modules/.bin/webpack & ./node_modules/.bin/uglifyjs --compress build/js/framework7.js",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --watch",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nolimits4web/Framework7.git"
  },
  "keywords": [
    "mobile",
    "framework",
    "ios 7",
    "ios7",
    "ios8",
    "ios 8",
    "iphone",
    "ipad",
    "apple",
    "phonegap",
    "native",
    "touch",
    "appstore",
    "app",
    "f7",
    "material",
    "android",
    "google",
    "googleplay"
  ],
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nolimits4web/Framework7/issues"
  },
  "homepage": "http://framework7.io/",
  "browserslist": [
    "Android > 4",
    "IOS >= 7",
    "last 2 versions",
    "ie >= 10"
  ],
  "dependencies": {
    "dom7": "^1.6.4",
    "global": "^4.3.2",
    "template7": "^1.2.5"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "diff": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "ts-loader": "^2.3.6",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "uglify-js": "^3.1.0",
    "webpack": "^3.5.6"
  }
}
